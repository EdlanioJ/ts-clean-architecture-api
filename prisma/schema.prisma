generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id           String   @id
  name         String
  username     String   @unique
  email        String   @unique
  phone_number String?  @unique
  password     String
  created_at   DateTime @default(now())
  updated_at   DateTime @default(now())
}
